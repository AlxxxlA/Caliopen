{
  "name": "caliopen.frontend",
  "version": "0.0.1",
  "description": "CaliOpen UI is the user interface for CaliOpen.",
  "homepage": "https://caliopen.org/",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development webpack-dashboard -- ./bin/server",
    "start:prod": "NODE_ENV=production ./bin/server",
    "start:desktop": "npm-run-all --parallel --race build:desktop:dev start:desktop:electron",
    "start:desktop:electron": "electron ./electron/",
    "start:smartphone": "npm-run-all --parallel build:smartphone:dev start:smartphone:android",
    "start:smartphone:android": "cd cordova && nodemon --watch www --exec 'cordova emulate android --debug'",
    "build:desktop:dev": "NODE_ENV=development webpack-dashboard -- webpack --watch --config webpack/webpack.config.electron.js",
    "build:smartphone:dev": "NODE_ENV=development webpack-dashboard -- webpack --watch --config webpack/webpack.config.cordova.js",
    "release": "webpack --config ./webpack/webpack.config.js",
    "cordova": "cd cordova && cordova"
  },
  "author": "CaliOpen <contact@caliopen.org> (https://caliopen.org/)",
  "license": "MIT",
  "dependencies": {
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "express": "^4.14.0",
    "npm-run-all": "^3.1.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "cordova": "^6.3.1",
    "electron": "^1.4.3",
    "html-webpack-plugin": "^2.22.0",
    "nodemon": "^1.11.0",
    "react-dev-utils": "^0.2.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
